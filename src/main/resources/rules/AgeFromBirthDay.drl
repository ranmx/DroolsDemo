package com.fosun

import com.fosun.tableObjects.*;

 
rule "age from birthday"
ruleflow-group "Group12"	
salience 10
    when  	
        m: YongAn(cleanedPerson.birthDay != null && cleanedPerson.age == 0)
    then
        m.cleanedPerson.age = 2017 - Integer.parseInt(m.cleanedPerson.birthDay.split("-")[0]);
        update(m);
end
