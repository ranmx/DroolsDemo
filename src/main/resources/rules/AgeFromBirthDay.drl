package com.fosun

import com.fosun.tableObjects.*;

 
rule "age from birthday"
ruleflow-group "Group2"	
salience 10
    when  	
        m: YongAn(cleanedPerson.getBirthDay() != null && cleanedPerson.getAge() == 0)
    then
        m.cleanedPerson.setAge(2017 - Integer.parseInt(m.cleanedPerson.getBirthDay().split("-")[0]));
        update(m);
end
